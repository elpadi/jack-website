{% macro menu_item(section) %}
<a class="plain" data-section="{{ section }}" href="{{ urlFor(section) }}">{{ section | capitalize }}</a>
{% endmacro %}
{% macro contact(name, position, email) %}
{{ name }}<br>
{{ position }}<br>
<a class="plain" href="mailto:{{ email }}">{{ email }}</a>
{% endmacro %}
